{
  "crons": [
    {
      "path": "/api/jobs/cron",
      "schedule": "0 10 * * *"
    }
  ],
  "functions": {
    "app/api/payment/razorpay/create-order/route.ts": {
      "maxDuration": 30
    },
    "app/api/payment/razorpay/verify/route.ts": {
      "maxDuration": 30
    }
  },
  "buildCommand": "npx prisma generate && npx prisma migrate deploy && npm run build",
  "env": {
    "PRISMA_QUERY_ENGINE_LIBRARY": "1"
  }
}


